        "print(\"\"\"\n",
        "      a - Ordenar de manera ascendente\n",
        "      b - Ordenar la lista de manera descendente\n",
        "      Ingrese una opción:\"\"\")\n",
        "op = input().lower()\n",
        "if op =='a':\n",
        "  alumnos.sort()\n",
        "elif op =='b':\n",
        "  alumnos.sort(reverse= True)\n",
        "else:\n",
        "  print(\"La opción ingresada no es correcta\")\n",
